<rdf:Description rdf:about="http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/ENG/40f410bd-8456-4393-94a9-99aeb9647a46">
	<rdf:type rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
