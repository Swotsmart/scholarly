	<authorityStatus xmlns="http://purl.org/ASN/schema/core/" rdf:resource="http://purl.org/ASN/scheme/ASNAuthorityStatus/Original"/>
	<hasLevel xmlns="http://purl.org/ASN/schema/core/" rdf:resource="http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/ART/f248f8f4-7506-4115-bdb2-f1f1e445acb7"/>
