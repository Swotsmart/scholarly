<rdf:Description rdf:about="http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/ART/7ec5fd09-f37c-44ca-ac99-05f3a954de77">
	<rdf:type rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
