	<hasChild xmlns="http://purl.org/gem/qualifiers/" rdf:resource="http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/SCI/af0c488f-8adf-44b0-80d6-ecea1a877a5b"/>
