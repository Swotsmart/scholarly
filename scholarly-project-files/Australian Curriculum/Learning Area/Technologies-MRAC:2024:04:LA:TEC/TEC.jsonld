artOf":[{"@id":"http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/TBD"}],"http://purl.org/dc/terms/modified":[{"@type":"http://www.w3.org/2001/XMLSchema#dateTime","@value":"2021-09-28T09:27:46Z"}],"http://purl.org/dc/terms/title":[{"@language":"en-au","@value":"using a variety of strategies such as brainstorming, sketching, 3D modelling and experimenting to generate innovative design ideas to present to others"}],"http://purl.org/gem/qualifiers/isChildOf":[{"@id":"http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/TEC/e94e0ed2-d6e5-40ad-848f-8b536e21adcf"}],"https://www.esa.edu.au/nominalYearLevel":[{"@language":"en-au","@value":"Year 7"}]},{"@id":"http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/TEC/d91e3bc2-59ae-41c3-919d-298843326e34","@type":["http://purl.org/ASN/schema/core/Statement","http://www.w3.org/2004/02/skos/core#Concept"],"http://purl.org/ASN/schema/core/authorityStatus":[{"@id":"http://purl.org/ASN/scheme/ASNAuthorityStatus/Original"}],"http://purl.org/ASN/schema/core/indexingStatus":[{"@id":"http://purl.org/ASN/scheme/ASNIndexingStatus/No"}],"http://purl.org/ASN/schema/core/statementLabel":[{"@language":"en-au","@value":"Achievement Standard"}],"http://purl.org/ASN/schema/core/statementNotation":[{"@language":"en-au","@value":"ASTECTDEY910"}],"http://purl.org/dc/terms/description":[{"@language":"en-au","@value":"By the end of Year 10 students explain how people consider factors that impact on design decisions and the technologies used to design and produce products, services and environments for sustainable living. They explain the contribution of innovation, enterprise skills and emerging technologies to global preferred futures. For one or more of the technologies contexts, students explain the features of technologies and their appropriateness for purpose, and create designed solutions based on an analysis of needs or opportunities. Students create, adapt and refine design ideas, processes and solutions and justify their decisions against developed design criteria that include sustainability. They communicate design ideas, processes and solutions to a range of audiences, including using digital tools. Students independently and collaboratively develop and apply production and project management plans, adjusting processes when necessary. They select and use technologies skilfully and safely to produce designed solutions."}],"http://purl.org/dc/terms/educationLevel":[{"@id":"http://vocabulary.curriculum.edu.au/schoolLevel/9"},{"@id":"http://vocabulary.curriculum.edu.au/schoolLevel/10"}],"http://purl.org/dc/terms/isPartOf":[{"@id":"http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/TBD"}],"http://purl.org/dc/terms/modified":[{"@type":"http://www.w3.org/2001/XMLSchema#dateTime","@value":"2021-09-29T14:07:16Z"}],"http://purl.org/dc/terms/title":[{"@language":"en-au","@value":"By the end of Year 10 students explain how people consider factors that impact on design decisions and the technologies used to design and produce products, services and environments for sustainable living. They explain the contribution of innovation, enterprise skills and emerging technologies to global preferred futures. For one or more of the technologies contexts, students explain the features of technologies and their appropriateness for purpose, and create designed solutions based on an analysis of needs or opportunities. Students create, adapt and refine design ideas, processes and solutions and justify their decisions against developed design criteria that include sustainability. They communicate design ideas, processes and solutions to a range of audiences, including using digital tools. Students independently and collaboratively develop and apply production and project management plans, adjusting processes when necessary. They select and use technologies skilfully and safely to produce designed solutions."}],"http://purl.org/gem/qualifiers/hasChild":[{"@id":"http://vocabulary.curriculum.edu.au/MRAC/2024/04/LA/TEC/5925c6df-2f